{
  "name": "@ibm/aiops-ui-extension-template",
  "version": "1.0.0",
  "description": "Dashboard extension examples and tools for Cloud Pak for AIOps",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "webpack-build": "webpack build --config webpack.config.js --mode production",
    "clean": "rimraf dist",
    "build": "npm-run-all clean webpack-build",
    "deploy": "npm-run-all build upload \"examples -- {@}\" --",
    "enable": "node ./enable.mjs",
    "disable": "node ./enable.mjs --disable",
    "remove": "node ./examples.mjs --remove",
    "examples": "node ./examples.mjs",
    "upload": "node ./deploy.mjs",
    "start": "webpack serve --watch --config webpack.config.js",
    "test": "cross-env TZ=utc jest --colors",
    "lint": "npm-run-all lint:ts lint:css",
    "lint:ts": "eslint src/**/*.{ts,tsx}",
    "lint:css": "stylelint src/**/*.{css,scss,sass}",
    "lint:css:fix": "stylelint src/**/*.{css,scss,sass} --fix",
    "build:storybook": "cross-env BABEL_ENV=storybook storybook build",
    "deploy:storybook": "storybook-to-ghpages -s build:storybook",
    "deploy:storybook:ci": "storybook-to-ghpages -s build:storybook --remote=\"deployorigin\"",
    "storybook": "cross-env storybook dev -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ibm/aiops-ui-extension-template.git"
  },
  "author": "IBM Corp.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/preset-env": "^7.23.3",
