{
  "name": "@ibm/aiops-ui-extension-template",
  "version": "1.0.0",
  "description": "Dashboard extension examples and tools for Cloud Pak for AIOps",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "webpack-build": "webpack build --config webpack.config.js --mode production",
    "clean": "rimraf dist",
    "build": "npm-run-all clean webpack-build",
    "deploy": "npm-run-all build upload \"examples -- {@}\" --",
    "enable": "node ./enable.mjs",
    "disable": "node ./enable.mjs --disable",
    "remove": "node ./examples.mjs --remove",
    "examples": "node ./examples.mjs",
    "upload": "node ./deploy.mjs",
    "start": "webpack serve --watch --config webpack.config.js",
    "test": "cross-env TZ=utc jest --colors",
    "lint": "npm-run-all lint:ts lint:css",
    "lint:ts": "eslint src/**/*.{ts,tsx}",
    "lint:css": "stylelint src/**/*.{css,scss,sass}",
    "lint:css:fix": "stylelint src/**/*.{css,scss,sass} --fix",
    "build:storybook": "cross-env BABEL_ENV=storybook storybook build",
    "deploy:storybook": "storybook-to-ghpages -s build:storybook",
    "deploy:storybook:ci": "storybook-to-ghpages -s build:storybook --remote=\"deployorigin\"",
    "storybook": "cross-env storybook dev -p 6006"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ibm/aiops-ui-extension-template.git"
  },
  "author": "IBM Corp.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@storybook/addon-a11y": "^8.6.9",
    "@storybook/addon-actions": "^7.5.3",
    "@storybook/addon-console": "^3.0.0",
    "@storybook/addon-essentials": "^8.6.9",
    "@storybook/addon-interactions": "^8.6.9",
    "@storybook/addon-links": "^8.6.9",
    "@storybook/builder-webpack5": "^8.6.9",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^8.6.9",
    "@storybook/react-webpack5": "^8.6.9",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/testing-library": "0.2.1",
    "@storybook/theming": "^8.6.9",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/carbon__icons-react": "11.26.2",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.22.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.0",
    "cp4waiops-ui-bundle-tools": "^0.0.4",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.2",
    "eslint": "^9.19.0",
    "eslint-plugin-formatjs": "^5.2.13",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.11.0",
    "file-loader": "^6.2.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-loader": "^7.0.2",
    "react": "^=18",
    "react-dom": "^=18",
    "rimraf": "^3.0.2",
    "sass": "^1.63.6",
    "sass-loader": "^10.2.1",
    "storybook": "^8.6.9",
    "storybook-addon-turbo-build": "^1.1.0",
    "style-loader": "^3.3.1",
    "stylelint": "14.15.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "svg-sprite-loader": "2.0.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@carbon/charts-react": "^1.17.2",
    "@carbon/colors": "^11.29.0",
    "@carbon/icons-react": "^11.24.0",
    "@carbon/react": "^1.42.1",
    "@carbon/styles": "^1.36.0",
    "@ibm/akora-renderer-react": "^1.0.4",
    "@kubernetes/client-node": "^0.19.0",
    "@storybook/test": "^8.6.9",
    "d3": "^7.8.5",
    "d3-cloud": "^1.2.7",
    "d3-sankey": "^0.12.3",
    "minimist": "^1.2.8"
  },
  "files": [
    "dist",
    "lang"
  ],
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "peerDependencies": {
    "react": "^=17",
    "react-dom": "^=17"
  },
  "overrides": {
    "git-url-parse": "^13.1.1",
    "postcss": "^8.4.31",
    "@floating-ui/react": "0.26.28"
  },
  "imports": {
    "#src/helpers/useQuery": {
      "storybook": "./src/helpers/useQuery.mock.ts",
      "default": "./src/helpers/useQuery.ts"
    },
    "#*": ["./*", "./*.ts", "./*.tsx"]
  }
}
